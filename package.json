{
  "name": "nord-bulma",
  "version": "0.1.0",
  "description": "Nord theme for Bulma",
  "main": "sass/nord-bulma.scss",
  "scripts": {
    "build": "node-sass --omit-source-map-url sass/nord-bulma.scss dist/nord-bulma.css --output-style compressed",
    "watch": "npm run build -- --watch",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sereneblue/nord-bulma.git"
  },
  "author": "sereneblue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sereneblue/nord-bulma/issues"
  },
  "homepage": "https://github.com/sereneblue/nord-bulma#readme",
  "devDependencies": {
    "bulma": "^0.7.4",
    "node-sass": "^4.12.0",
    "nord": "^0.2.1"
  }
}
